{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MessageBrokerConfiguration": {
    "BrokerName": "RabbitMQ",
    "Host": "localhost",
    "Port": "5672",
    "UserName": "guest",
    "Password": "guest",
    "ErrorQueue": "error",
    "ErrorCriticalQueue": "error_critical"
  },
  "ServiceRegistryConfiguration": {
    "BaseUrl": "http://localhost:3018",
    "Endpoints": [
      {
        "Name": "Discovery",
        "Route": "/v1/service/{serviceName}/{endpointName}",
        "Method": "GET",
        "RequiredAuthorization": false
      },
      {
        "Name": "ListEndpoints",
        "Route": "/v1/service/{serviceName}",
        "Method": "GET",
        "RequiredAuthorization": false
      }
    ]
  },
  "AllowedHosts": "*"
}
